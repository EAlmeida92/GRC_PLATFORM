@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <script>
function subst() {
    var vars={};
    var x=document.location.search.substring(1).split('&');
    for(var i in x) {var z=x[i].split('=',2);vars[z[0]] = unescape(z[1]);}
    var x=['frompage','topage','page','webpage','section','subsection','subsubsection'];
    for(var i in x) {
        var y = document.getElementsByClassName(x[i]);
        for(var j=0; j<y.length; ++j) y[j].textContent = vars[x[i]];
    }
    if (vars['page'] == 1) { // If page is 1, set FakeHeaders display to none
        document.getElementById("corpo_rodape").style.display = 'none';
    }

}


    </script>

</head>
<body id="corpo_rodape" style="border: 0; margin: 0;" onload="subst()">

    <table style="width: 100%; height:55px;border-top:1pt solid black" ">
        <tr>
            <td style="width: 25%" align="left">di4you.pt</td>
            <td style="width: 50%" align="center">Pagina <span class="page"></span> de <span class="topage"></span></td>
            <td style="width: 25%" align="right"><div id="date"></div></td>
        </tr>
        <tr>
            <td colspan="3" align="right"><p></p></td>

        </tr>

    </table>

</body>
</html>
<script>
n = new Date();
y = n.getFullYear();
m = n.getMonth() + 1;
d = n.getDate();
document.getElementById("date").innerHTML = "DATA : "+ d + "/" + m + "/" + y;
</script>
